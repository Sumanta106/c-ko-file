#include<stdio.h>
void main()
{
	int num[5];
	int price[5],total=0;
	for(int i=0;i<5;i++)
	{
		printf("enter the nbr of bulb:\n");
		scanf("%d",&num[i]);
		printf("enter the price of bulb:\n");
		scanf("%d",&price[i]);
	}
	for(int i=0;i<5;i++)
	{
		total=total+num[i]*price[i];
	}
	printf("the price is %d",total);
}

	

